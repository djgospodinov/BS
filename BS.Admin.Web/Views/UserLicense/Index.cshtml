@model int
<h2>Фирмени данни</h2>
<br />
@if (BS.Admin.Web.RolesManager.CanCreateLicense(User.Identity))
{
    @Html.ActionLink("Създай потребител/фирма", "Create", "UserLicense")
}

<br />
<br />
<br />

<div id="tabs">
    <ul>
        <li><a href="#tabs-1">Реални</a></li>
        <li><a href="#tabs-2">Демо</a></li>
    </ul>
    <div id="tabs-1">
        @{ Html.RenderPartial("UsersTable", new BS.Admin.Web.Models.UserLicenseGridModel() { Demo = false }); }
    </div>
    <div id="tabs-2">
        @{ Html.RenderPartial("UsersTable", new BS.Admin.Web.Models.UserLicenseGridModel() { Demo = true }); }
    </div>
</div>

<script>
    $(function () {
        $("#tabs").tabs({ active: @Model });
    });
</script>